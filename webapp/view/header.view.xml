<mvc:View xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc"
   xmlns:core="sap.ui.core" xmlns:tnt="sap.tnt" controllerName="progresstoolsTeam4progresstools.controller.header">
	<tnt:ToolHeader>
	<IconTabHeader id="iconTabHeader" selectedKey="invalidKey" select="onSelectTab" showOverflowSelectList="true" backgroundDesign="Transparent"
		mode="Inline">
		<layoutData>
			<OverflowToolbarLayoutData shrinkable="true"/>
		</layoutData>
		<items>
			<IconTabFilter text="Home" key="0"></IconTabFilter>
			<IconTabFilter text="Courses" key="1"></IconTabFilter>
			<IconTabFilter text="Progression Plan" key="2" ></IconTabFilter>
		</items>
	</IconTabHeader>
	<ToolbarSpacer>
		<layoutData>
			<OverflowToolbarLayoutData minWidth="16px"/>
		</layoutData>
	</ToolbarSpacer>
	<MenuButton icon="https://png.icons8.com/office/80/000000/worker-male.png" type="Transparent">
		<layoutData>
			<OverflowToolbarLayoutData/>
		</layoutData>
		<menu>
			<Menu>
				<items>
					<MenuItem text="Profile" icon="sap-icon://account" press="goToEdit"/>
					<MenuItem text="Logout" icon="sap-icon://log"/>
				</items>
			</Menu>
		</menu>
	</MenuButton>
</tnt:ToolHeader>
<App id="app"></App>
</mvc:View>