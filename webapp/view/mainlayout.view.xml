<mvc:View controllerName="progresstoolsTeam4progresstools.controller.mainlayout" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m" xmlns:tnt="sap.tnt">
	<App>
		<pages>
			<Page title="{i18n>title}">
				<content>
					<tnt:ToolHeader>
		<Button icon="sap-icon://home" type="Transparent" press="onHomePress">
			<layoutData>
				<OverflowToolbarLayoutData />
			</layoutData>
		</Button>
		<IconTabHeader id="iconTabHeader"
			selectedKey="invalidKey"
			select="onSelectTab"
			showOverflowSelectList="true"
			backgroundDesign="Transparent"
			mode="Inline">
			<layoutData>
				<OverflowToolbarLayoutData
					shrinkable="true" />
			</layoutData>
			<items>
				<IconTabFilter text="Documentation">
				</IconTabFilter>
				<IconTabFilter text="Explored">
				</IconTabFilter>
				<IconTabFilter text="API Reference">
				</IconTabFilter>
				<IconTabFilter text="Demo Apps">
				</IconTabFilter>
			</items>
		</IconTabHeader>

		<ToolbarSpacer>
			<layoutData>
				<OverflowToolbarLayoutData minWidth="16px" />
			</layoutData>
		</ToolbarSpacer>
		<Button icon="sap-icon://search" type="Transparent">
			<layoutData>
				<OverflowToolbarLayoutData />
			</layoutData>
		</Button>
		<Button icon="sap-icon://comment" type="Transparent">
			<layoutData>
				<OverflowToolbarLayoutData />
			</layoutData>
		</Button>

		<MenuButton icon="sap-icon://hint" type="Transparent">
			<layoutData>
				<OverflowToolbarLayoutData />
			</layoutData>
			<menu>
				<Menu>
					<items>
						<MenuItem text="Edit" icon="sap-icon://edit" />
						<MenuItem text="Save" icon="sap-icon://save" />
					</items>
				</Menu>
			</menu>
		</MenuButton>

	</tnt:ToolHeader>
	<Panel>
		<Label
			id="labelId"
			text="Home Screen"/>
	</Panel>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>