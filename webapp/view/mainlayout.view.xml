			<layoutData>
				<OverflowToolbarLayoutData priority="Low" />
			</layoutData>
		</Button>
		<Button text="Run" type="Transparent">
			<layoutData>
				<OverflowToolbarLayoutData priority="Low" />
			</layoutData>
		</Button>
		<Button text="Tools" type="Transparent">
			<layoutData>
				<OverflowToolbarLayoutData priority="Low" />
			</layoutData>
		</Button>
		<tnt:ToolHeaderUtilitySeparator />
		<ToolbarSpacer>
			<layoutData>
				<OverflowToolbarLayoutData minWidth="20px" />
			</layoutData>
		</ToolbarSpacer>
		<Button text="Alan Smith" type="Transparent" press="onUserNamePress">
			<layoutData>
				<OverflowToolbarLayoutData />
