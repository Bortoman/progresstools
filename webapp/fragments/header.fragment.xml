<core:FragmentDefinition xmlns="sap.m"
   xmlns:core="sap.ui.core" xmlns:tnt="sap.tnt">
	<tnt:ToolHeader>
	<Button icon="sap-icon://home" type="Transparent" press="onHomePress">
		<layoutData>
			<OverflowToolbarLayoutData/>
		</layoutData>
	</Button>
	<IconTabHeader id="iconTabHeader" selectedKey="invalidKey" select="onSelectTab" showOverflowSelectList="true" backgroundDesign="Transparent"
		mode="Inline">
		<layoutData>
			<OverflowToolbarLayoutData shrinkable="true"/>
		</layoutData>
		<items>
			<IconTabFilter text="Courses"></IconTabFilter>
			<IconTabFilter text="Progression Plan"></IconTabFilter>
		</items>
	</IconTabHeader>
	<ToolbarSpacer>
		<layoutData>
			<OverflowToolbarLayoutData minWidth="16px"/>
		</layoutData>
	</ToolbarSpacer>
	<MenuButton icon="https://png.pngtree.com/element_origin_min_pic/17/09/01/75775d69e115fe17a43c29ff3c5149be.jpg" type="Transparent">
		<layoutData>
			<OverflowToolbarLayoutData/>
		</layoutData>
		<menu>
			<Menu>
				<items>
					<MenuItem text="Edit" icon="sap-icon://edit"/>
					<MenuItem text="Logout" icon="sap-icon://log"/>
				</items>
			</Menu>
		</menu>
	</MenuButton>
</tnt:ToolHeader>
</core:FragmentDefinition>